<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="GreenSmart_interfaces.Inicio"
             Title="Inicio"
             BackgroundColor="#B7ECCA">

    <VerticalStackLayout>


        <Label Text="GreenSmart" FontSize="24" FontAttributes="Bold" TextColor="White" HorizontalOptions="Center"
               Margin="0,35,0,0"/>

        <Image Source="https://images.vexels.com/media/users/3/201075/isolated/preview/b893c17e341ed4f2734565958a642a77-planta-en-icono-de-maceta.png" HeightRequest="120" WidthRequest="120" HorizontalOptions="Center"
         />

        <Grid ColumnDefinitions="*,*" RowDefinitions="*,*" Margin="0,55,0,0">
            
            <Button Text="Invernaderos" Grid.Column="0" Grid.Row="0" CornerRadius="20" 
                    BackgroundColor="White" WidthRequest="150" HeightRequest="120"  ImageSource="data:image/png;base64,"
                    ContentLayout="Top,0"/>
            

            <Button Text="Registrar invernaderos" Grid.Column="1" Grid.Row="0" CornerRadius="20" 
                    BackgroundColor="White" WidthRequest="150" HeightRequest="120"  ImageSource="data:image/png;base64,"
                    ContentLayout="Top,0"/>

            <Button Text="Estadisticas" Grid.Column="0" Grid.Row="1" CornerRadius="20" 
        BackgroundColor="White" WidthRequest="150" HeightRequest="120"  ImageSource="https://cdn-icons-png.flaticon.com/512/3798/3798510.png"
        ContentLayout="Top,10"/>
            
            <Button Text="Humedad" Grid.Column="1" Grid.Row="1" CornerRadius="20" 
                    BackgroundColor="White" WidthRequest="150" HeightRequest="120" Margin="0,35,0,0" ImageSource="https://images.vexels.com/media/users/3/201258/isolated/preview/272ea9d721b36d1c8826549e8a90a7cb-icono-de-brote-de-riego.png?w=360"
                    ContentLayout="Top,10"/>
        </Grid>

    </VerticalStackLayout>
</ContentPage>